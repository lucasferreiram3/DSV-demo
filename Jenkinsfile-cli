pipeline {
    agent any
    stages {
        stage("Read DSV secrets") {
            steps {
                   //sh 'ls -l'
                   //sh './dsv auth >> /dev/null'
                   //sh './dsv secret read secrets:demo2 --filter ".attributes.password"'    
                   DSV_SECRET_PASS = sh(script: "dsv secret read secrets:demo2 --filter .attributes.password", returnStdout: true).trim()
                   sh 'echo "${DSV_SECRET_PASS}"' 
            }
        }
    }
}