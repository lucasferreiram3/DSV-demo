pipeline {
    agent any
    stages {
        stage("Read DSV secrets") {
            steps {
                   //sh 'ls -l'
                sh './dsv auth >> /dev/null'
                   //sh 'dsv secret read secrets:demo2 --filter .attributes.password'
                sh 'SECRET1="(dsv secret read secrets:demo2 --filter .attributes.password)"'
                echo "Secret from DSV is = ${SECRET1}"
            }
        }
    }
}