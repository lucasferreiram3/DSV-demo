pipeline {
    agent any
    stages {
        stage("Read DSV secrets") {
            steps {
                withCredentials([usernamePassword(credentialsId: 'dsv-jenkins', passwordVariable: 'PASS', usernameVariable: 'USER')]) {
                   sh 'dsv secret search --auth-username '${PASS}' --auth-password'${USER}''    
                }
                
            }
        }
    }
}